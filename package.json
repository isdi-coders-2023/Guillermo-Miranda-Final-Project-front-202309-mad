{
  "name": "final-project-front",
  "version": "1.0.0",
  "description": "Full stack project",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "server": "json-server --watch ./server/db.json",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:ts": "tsc -w",
    "test": "jest",
    "test:pro": "jest --watchAll --coverage --watchAll=false --passWithNoTests",
    "test:w": "jest --watchAll --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "test:f": "jest --watchAll --coverage",
    "prepare": "husky install",
    "sass": "sass scss:css -w --no-source-map",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "api": "json-server --watch ./api/db.json"
  },
  "keywords": [],
  "author": "Guillermo Miranda",
  "license": "ISC",
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "sass": "^1.69.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "@types/redux-mock-store": "^1.0.6",
    "redux-mock-store": "^1.5.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "chalk": "^5.3.0",
    "json-server": "^0.17.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.18.0",
    "sweetalert2": "^11.10.1"
